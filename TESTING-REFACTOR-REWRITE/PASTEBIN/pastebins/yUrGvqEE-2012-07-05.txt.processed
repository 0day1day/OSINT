/*-----------------------
	Nav Block
-----------------------*/
nav {
	padding:9px 0;
	float:left;
	width:980px;
}
nav ul {
	float: left;
	width: 100%;
}

nav li {
	float: left;
}

nav a {
	display: block;
	padding: 6px 10px 7px;
	background: none;
	color: #007dc3;
	text-decoration: none;
}

nav.main li {
	width:20%;
	text-align:center;
}

nav.main a {
	font-weight:bold;
	color:#007dc3;
}

nav.main a:hover, nav.main a:focus, nav.main li.active a { text-decoration: none; }

/*-------- Sub Navs --------*/

nav a:hover, nav a:focus, nav li.active a { background: #fff;color: #00549e;text-decoration: underline;}

nav.utility_nav a, nav.footer_nav a { background: none;font-size: 12px;padding: 2px 6px;color: #00549e;line-height: 1; font-size: 11px;  }

nav.utility_nav { margin: 6px 0;position: absolute;top: -28px;right: 0;width: 972px;}
nav.utility_nav li { float: right; }
nav.utility_nav li.first { margin-right: 0; }
nav.utility_nav a { border-left:1px solid #00549e;}
nav.utility_nav .first a { border-left: none; }

nav.footer_nav a { border-right:1px solid #888; color: #8c8c8c; font-weight: bold; font-size:10px; }

/*-------- Search --------*/

#search { position: absolute; top: 19px; right: 20px; margin-right: -5px; }

#search input { padding: 2px; border:0; }

.search_button { width: 27px; height: 28px; background:transparent url('/assets/images/css/search_btn.gif') no-repeat 0 0; border:none; text-indent: -99999px; position: relative; left: -5px; }


/*-------- Highlights --------*/


.highlight h1 { color: #fff797; background: #fff url('/assets/images/css/highlight_bg_l.png') no-repeat 0 0; padding-left: 24px; text-transform: uppercase; height:26px; }
.highlight h1 span { display: inline-block; background: #8c9957 url('/assets/images/css/highlight_bg_r.png') no-repeat 100% 0; padding-right:40px; line-height:26px; }


.lt_3 { margin-left: 1em; list-style-type: none; margin-top:10px; }
.lt_3 li { line-height: 2; }
.lt_3 a { color: #007dc3; text-decoration: none; font-size: 12px; font-weight: bold; }
.lt_3 a:hover, .highlight_list a:focus, .highlight_list a:active { text-decoration: underline; }

.lt_4 { margin-left: 0; list-style-type: none; margin-top:10px; }
